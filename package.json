		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.37.3",
		"nanoid": "^4.0.2",
		"nprogress": "^0.2.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persist": "^1.0.0",
		"qs": "^6.11.2",
		"reconnecting-websocket": "^4.4.0",
		"store": "^2.0.12",
